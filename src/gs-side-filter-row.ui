<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.10 -->
  <template class="GsSideFilterRow" parent="GtkListBoxRow">
    <property name="visible">True</property>
    <child>
      <object class="GtkBox" id="outterbox">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="orientation">horizontal</property>
        <child>
          <object class="GtkBox" id="leftborder">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="width-request">8</property>
            <style>
              <class name="filter-left-border"/>
            </style>
          </object>
        </child>
	<child>
	  <object class="GtkBox" id="box">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="orientation">horizontal</property>
            <property name="spacing">12</property>
            <property name="margin_top">12</property>
            <property name="margin_bottom">12</property>
            <property name="margin_start">9</property>
            <property name="margin_end">9</property>
            <child>
              <object class="GtkImage" id="image">
		<property name="visible">True</property>
		<property name="can_focus">False</property>
		<property name="icon_name">folder-music-symbolic</property>
		<property name="icon_size">1</property>
		<style>
		  <class name="dim-label"/>
		</style>
	      </object>
            </child>
            <child>
              <object class="GtkLabel" id="label">
		<property name="visible">True</property>
		<property name="xalign">0</property>
		<attributes>
		  <attribute name="weight" value="bold"/>
		</attributes>
		<style>
		  <class name="dim-label"/>
		</style>
              </object>
            </child>
	  </object>
	</child>
      </object>
    </child>
  </template>
</interface>
